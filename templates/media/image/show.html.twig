{% extends 'base.html.twig' %}

{% block title %}Image{% endblock %}

{% block body %}
    <h1>Image</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ image.id }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ image.reference }}</td>
            </tr>
            <tr>
                <th>OriginalFileName</th>
                <td>{{ image.originalFileName }}</td>
            </tr>
            <tr>
                <th>FilePath</th>
                <td>{{ image.filePath }}</td>
            </tr>
            <tr>
                <th>Width</th>
                <td>{{ image.width }}</td>
            </tr>
            <tr>
                <th>Height</th>
                <td>{{ image.height }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('media_image_index') }}">back to list</a>

    <a href="{{ path('media_image_edit', {'id': image.id}) }}">edit</a>

    {{ include('media_image/_delete_form.html.twig') }}
{% endblock %}
